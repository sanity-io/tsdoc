{
  "name": "multi-export",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/exports/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./extra": {
      "types": "./dist/exports/extra.d.ts",
      "source": "./src/extra.ts",
      "require": "./dist/extra.cjs",
      "import": "./dist/extra.js",
      "default": "./dist/extra.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/exports/index.d.ts",
  "typesVersions": {
    "*": {
      "extra": [
        "./dist/exports/extra.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rimraf dist && pkg build --strict && pkg --strict",
    "etl": "sanity-tsdoc etl --outDir ../../etc"
  },
  "devDependencies": {
    "@sanity/tsdoc": "workspace:*"
  }
}
