{
  "name": "@sanity/tsdoc",
  "version": "1.0.8",
  "description": "Generate API reference docs from TypeScript projects and store in a Sanity-friendly JSON format. Render a static frontend, or as React components.",
  "keywords": [
    "sanity",
    "TSDoc",
    "TypeScript",
    "API",
    "API reference",
    "generate",
    "documentation"
  ],
  "homepage": "https://www.sanity.io/",
  "bugs": {
    "url": "https://github.com/sanity-io/tsdoc/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/tsdoc.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "exports": {
    ".": {
      "source": "./exports/index.ts",
      "import": "./dist/exports/index.mjs",
      "require": "./dist/exports/index.js",
      "default": "./dist/exports/index.js"
    },
    "./react": {
      "source": "./exports/react.ts",
      "import": "./dist/exports/react.mjs",
      "require": "./dist/exports/react.js",
      "default": "./dist/exports/react.js"
    },
    "./store": {
      "source": "./exports/store.ts",
      "import": "./dist/exports/store.mjs",
      "require": "./dist/exports/store.js",
      "default": "./dist/exports/store.js"
    },
    "./studio": {
      "source": "./exports/studio.ts",
      "import": "./dist/exports/studio.mjs",
      "require": "./dist/exports/studio.js",
      "default": "./dist/exports/studio.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/exports/index.js",
  "module": "./dist/exports/index.esm.js",
  "source": "./exports/index.ts",
  "types": "./dist/exports/index.d.ts",
  "typesVersions": {
    "*": {
      "react": [
        "./dist/exports/react.d.ts"
      ],
      "store": [
        "./dist/exports/store.d.ts"
      ],
      "studio": [
        "./dist/exports/studio.d.ts"
      ]
    }
  },
  "bin": {
    "sanity-tsdoc": "./bin/sanity-tsdoc"
  },
  "files": [
    "bin",
    "dist",
    "exports",
    "src"
  ],
  "scripts": {
    "build": "run-s clean pkg:build pkg:check",
    "clean": "npx rimraf dist react.js store.js studio.js",
    "commit": "cz",
    "dev": "run-p *:dev watch",
    "etl": "node -r dotenv-flow/config -r esbuild-register scripts/etl",
    "format": "prettier --cache --write .",
    "lint": "eslint --cache . --ext .cjs,.js,.ts,.tsx",
    "lint:fix": "pnpm format && pnpm lint --fix --quiet",
    "pkg:build": "pkg build",
    "pkg:check": "pkg --strict",
    "prepublishOnly": "run-s build",
    "release": "semantic-release",
    "test": "vitest",
    "tsdoc:dev": "node -r dotenv-flow/config -r esbuild-register scripts/dev",
    "type-check": "tsc --build",
    "watch": "pkg watch",
    "workshop:build": "workshop build --cwd workshop",
    "workshop:dev": "workshop dev --cwd workshop"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "plugins": [
      "prettier-plugin-packagejson"
    ],
    "printWidth": 100,
    "semi": false,
    "singleQuote": true
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "extends": "@sanity/semantic-release-preset"
  },
  "dependencies": {
    "@microsoft/api-extractor": "7.43.0",
    "@microsoft/api-extractor-model": "7.28.13",
    "@microsoft/tsdoc": "0.14.2",
    "@microsoft/tsdoc-config": "0.16.2",
    "@portabletext/react": "^3.0.15",
    "@portabletext/toolkit": "^2.0.13",
    "@sanity/client": "^6.15.9",
    "@sanity/color": "^3.0.4",
    "@sanity/icons": "^2.11.6",
    "@sanity/pkg-utils": "5.1.8",
    "@sanity/ui": "^2.0.14",
    "@types/cpx": "^1.5.5",
    "@vitejs/plugin-react": "^4.2.1",
    "cac": "^6.7.14",
    "chalk": "^4.1.2",
    "chokidar": "^3.6.0",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "esbuild": "^0.20.2",
    "esbuild-register": "^3.5.0",
    "express": "^4.19.2",
    "globby": "^11.1.0",
    "groq": "^3.36.4",
    "groq-js": "^1.7.0",
    "history": "^5.3.0",
    "jsonc-parser": "^3.2.1",
    "mkdirp": "^1.0.4",
    "pkg-up": "^3.1.0",
    "react-refractor": "^2.1.7",
    "slugify": "^1.6.6",
    "tmp": "^0.2.3",
    "typescript": "5.4.2",
    "vite": "^5.2.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@sanity/semantic-release-preset": "^4.1.7",
    "@sanity/ui-workshop": "^2.0.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^20.2.3",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@types/tmp": "^0.2.6",
    "@types/uuid": "^9.0.8",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "commitizen": "^4.3.0",
    "cpx": "^1.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.4.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "refractor": "^4.8.1",
    "rimraf": "^4.4.1",
    "sanity": "^3.36.4",
    "semantic-release": "^23.0.7",
    "styled-components": "^6.1.8",
    "uuid": "^9.0.1",
    "vitest": "^1.4.0",
    "vitest-github-actions-reporter": "^0.11.1"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18",
    "sanity": "^3",
    "styled-components": "^5.2 || ^6"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "overrides": {
      "@sanity/tsdoc": "workspace:*",
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  }
}
