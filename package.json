{
  "name": "@sanity/tsdoc",
  "version": "0.4.1",
  "sideEffects": false,
  "source": "./src/tsdoc.ts",
  "module": "./dist/tsdoc.js",
  "types": "./dist/types/src/tsdoc.d.ts",
  "main": "./dist/tsdoc.cjs",
  "exports": {
    ".": {
      "source": "./src/tsdoc.ts",
      "import": "./dist/tsdoc.js",
      "types": "./dist/types/src/tsdoc.d.ts",
      "require": "./dist/tsdoc.cjs"
    },
    "./react": {
      "source": "./src/react.ts",
      "import": "./dist/react.js",
      "types": "./dist/types/src/react.d.ts",
      "require": "./dist/react.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "react": [
        "./dist/types/src/react.d.ts"
      ]
    }
  },
  "bin": {
    "sanity-tsdoc": "./bin/sanity-tsdoc",
    "tsdoc": "./bin/sanity-tsdoc"
  },
  "files": [
    "bin",
    "dist",
    "src"
  ],
  "license": "MIT",
  "scripts": {
    "build": "pkg build --tsconfig tsconfig.dist.json",
    "clean": "rimraf dist",
    "commit": "cz",
    "dev": "node -r esbuild-register scripts/dev",
    "etl": "node -r esbuild-register scripts/etl",
    "lint": "eslint . --ext .cjs,.js,.ts,.tsx",
    "prebuild": "yarn clean",
    "prepublishOnly": "pnpm run build",
    "release": "semantic-release",
    "test": "pkg --strict && jest",
    "type-check": "tsc --build"
  },
  "dependencies": {
    "@microsoft/api-extractor": "^7.30.0",
    "@microsoft/api-extractor-model": "^7.24.0",
    "@microsoft/tsdoc": "^0.14.1",
    "@microsoft/tsdoc-config": "^0.16.1",
    "@portabletext/react": "^1.0.6",
    "@sanity/icons": "^1.3.6-next.14",
    "@sanity/ui": "^0.38.2-next.14",
    "@types/cpx": "^1.5.2",
    "@vitejs/plugin-react": "^2.1.0",
    "chalk": "^4.1.2",
    "groq": "^2.29.3",
    "groq-js": "^1.0.1",
    "history": "^5.3.0",
    "jsonc-parser": "^3.2.0",
    "mkdirp": "^1.0.4",
    "pkg-up": "^3.1.0",
    "react-refractor": "^2.1.7",
    "refractor": "^4.8.0",
    "slugify": "^1.6.5",
    "tmp": "^0.2.1",
    "vite": "^3.1.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@sanity/semantic-release-preset": "^2.0.0",
    "@sanity/pkg-utils": "^1.6.0",
    "@types/jest": "^29.0.0",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^18.7.16",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@types/tmp": "^0.2.3",
    "@types/yargs": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "commitizen": "^4.2.5",
    "cpx": "^1.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.15.7",
    "esbuild-jest": "^0.5.0",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.7",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^29.0.2",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.5",
    "styled-components": "^5.3.5",
    "typescript": "^4.8.2"
  },
  "peerDependencies": {
    "@sanity/ui": "^0.38",
    "react": "^18",
    "react-dom": "^18",
    "react-is": "^18",
    "styled-components": "^5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/tsdoc.git"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/tsdoc/issues"
  },
  "keywords": [
    "sanity",
    "ui",
    "components"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "homepage": "https://www.sanity.io/",
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ]
  }
}
