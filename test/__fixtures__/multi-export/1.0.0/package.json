{
  "type": "module",
  "name": "multi-export",
  "version": "1.0.0",
  "license": "MIT",
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/dts/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "require": "./lib/cjs/index.js",
      "default": "./lib/esm/index.js",
      "types": "./lib/dts/src/index.d.ts"
    },
    "./extra": {
      "source": "./src/extra.ts",
      "require": "./lib/cjs/extra.js",
      "default": "./lib/esm/extra.js",
      "types": "./lib/dts/src/extra.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "extra": [
        "./lib/dts/src/extra.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.lib.json && yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc --project tsconfig.lib.json --outDir lib/cjs",
    "build:esm": "tsc --project tsconfig.lib.json --outDir lib/esm",
    "prebuild": "rimraf lib"
  },
  "devDependencies": {
    "@types/react": "^18.0.18",
    "react": "^18.2.0",
    "typescript": "^4.8.2"
  },
  "bundledDependencies": [
    "multi-export"
  ]
}
